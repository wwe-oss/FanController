{
    "C_Cpp.default.configurationProvider": "ms-vscode.cpptools",
    "C_Cpp.intelliSenseEngine": "default",
    "files.associations": {
        "*.ino": "cpp"
    },
    "platformio-ide.useBuiltinPIOCore": true,
    "chat.disableAIFeatures": true,
    "githubPullRequests.codingAgent.enabled": false,
    "githubPullRequests.experimental.chat": false,
    "genaiscript.githubCopilotInstructions": false,
    "chatgpt.runCodexInWindowsSubsystemForLinux": true,
    "C_Cpp.formatting": "clangFormat",
    "gitlab.pipelineGitRemoteName": "https://gitlab.com/wwe-oss/",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "notebook.defaultFormatter": "ms-vscode.cmake-tools"
}
